[run]
omit =
    */tests.py
    */test_*.py
    */tests/*
    */venv/*
    */virtualenv/*
    */env/*
    manage.py
    */migrations/*
    */settings.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

